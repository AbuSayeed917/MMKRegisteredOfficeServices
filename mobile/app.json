{
  "expo": {
    "name": "MMK Office",
    "slug": "mmk-office",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "mmkoffice",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0c2d42"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mmkaccountants.office"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0c2d42"
      },
      "package": "com.mmkaccountants.office",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#007AFF"
        }
      ]
    ]
  }
}
